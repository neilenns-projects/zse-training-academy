{
  "files.exclude": {
    ".hugo_build.lock": true,
    "public/": true,
    "resources/": true,
    "design-files": false,
    "package*.json": true,
    "**/*-backups": true
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".gitignore": ".gitattributes, .hugoignore",
    "hugo.yaml": ".markdownlint.json, go.mod, go.sum, .coderabbit.yml, .prettierrc.json",
    "eslint.config.js": ".markdownlint.jsonc",
    "README.md": "DEVELOPMENT.md, DEPLOYMENT.md"
  },
  "files.associations": {
    "*.svg.html": "plaintext"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "cSpell.words": [
    "AIRMET",
    "AIRMETs",
    "ALTRV",
    "ARTCC",
    "ATIS",
    "callsign",
    "Coupeville",
    "coupville",
    "cruiseid",
    "CYVR",
    "Enroute",
    "Hillsboro",
    "KHIO",
    "KJFK",
    "KPDX",
    "KSEA",
    "KYKM",
    "METAR",
    "MOCA",
    "navaid",
    "NAVAIDs",
    "OROCA",
    "PIREP",
    "RNAV",
    "RVSM",
    "SEDTO",
    "SIGMET",
    "SIGMETs",
    "SPECI",
    "SVFR",
    "TACAN",
    "TDLS",
    "TRSA",
    "VATSIM",
    "VATUSA"
  ],
  "svg.preview.background": "white"
}
